alphabet = ${alphabet}
idx = -1
input_str = ''

def read_char():
    global idx
    idx += 1
    if idx < len(input_str):
        c = input_str[idx]
    else:
        return ''
    if not c in alphabet:
        raise Exception('Error!Illegal character!')
    return c

def undo_read():
    global idx
    if idx < len(input_str):
        idx -= 1

def next():
    c = read_char()
    undo_read()
    return c

def execute():
    global input_str
    input_str = input()
    s = '${start}'